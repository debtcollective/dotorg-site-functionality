{
    "name": "debtcollective/site",
    "description": "Debt Collecive plugins and theme",
    "type": "project",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
              "name": "advanced-custom-fields/advanced-custom-fields-pro",
              "version": "*.*.*(.*)",
              "type": "wordpress-plugin",
              "dist": {
                "type": "zip",
                "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
                },
            "require": {
                "philippbaschke/acf-pro-installer": "^1.0",
                "composer/installers": "^1.0"
                }
            }
        },
        {
            "type": "github",
            "name": "communiteq/wp-discourse-shortcodes",
            "url":  "git@github.com:communiteq/wp-discourse-shortcodes.git"
        }
    ],
    "require": {
        "philippbaschke/acf-pro-installer": "^1.0",
        "advanced-custom-fields/advanced-custom-fields-pro": "*",
        "wpackagist-plugin/getwid":"^1.7.3",
        "wpackagist-plugin/members":"^3.1.5",
        "wpackagist-plugin/page-links-to":"^3.3.5",
        "wpackagist-plugin/wp-discourse":"^2.3.0",
        "communiteq/wp-discourse-shortcodes": "*",
        "wpackagist-plugin/wp-graphql":"^1.6.2",
        "wpackagist-plugin/wp-rest-api-v2-menus":"^0.10",
        "deliciousbrains-plugin/wp-offload-media": "^2.0"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
